@using Microsoft.AspNetCore.Mvc.ModelBinding.Binders
@model IQueryable<Bookish.Models.Member>

@{
    ViewBag.Title = "Member Query Display";
}
<h3>Member Info</h3>
<div class="row">
    <table style="width:100%">
        <tr>
            <th>Member ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>View details</th>
            
        </tr>

        @{
            foreach (var member in Model)
            {
                <tr>
                    <td>@member.MemberId</td>
                    <td>@member.Name</td>
                    <td>@member.Email</td>
                    <td>@Html.ActionLink("View","MemberQuery",member,FormMethod.Get)</td>
                </tr>
            }
        }

    </table>
    <p></p>
</div>
    
    
  

